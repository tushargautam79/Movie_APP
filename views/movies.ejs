              <div>
                <p style="display:inline-block;">Email id :- </p>
                <input type="email" name="email" placeholder="tushargautam79@gmail.com" id="signup_modal" required="">
              </div>
              <div>
                <p style="display:inline-block;">User name :- </p>
                <input type="text" name="username" placeholder="tushargautam79" id="signup_modal" required="">
              </div>
              <div>
                <p style="display:inline-block;">password :- </p>
                <input type="password"  placeholder="12345678@25" id="signup_modal1" required="">
              </div>
              <div>
