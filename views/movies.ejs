    </div>
    <div class="row ">
      <div class="col-4">
        <div id="side_bar">
          <h1 id="head">Top Hit</h1>
          <ul>
            <%for(var k=0;k< web.length;k++){%>
              <li><%=web[k]["Title"]%></li>
              <%}%>
          </ul>
        </div>
      </div>
      <div class="col-6"> 
        <%for(var k=0;k< web.length;k++){%>
          <div id="content" class="row">
            <div class="col-6">
               <img id="poster" src= <%=web[k]["Poster"]%> >
            </div>
            <div id="detail" class="col-6" style="color: white">
              <h1 style="color: #4CAF50"><%=web[k]["Title"]%></h1>
              <h2>Year of Release: <%=web[k]["Year"]%></h2>
              <h2>Type: <%=web[k]["Type"]%></h2>
              <h2>IMBD Id: <%=web[k]["imdbID"]%></h2>
                <% var mk="http://localhost:3000/id/"+web[k]["imdbID"] %>
                <a  href= <%=mk%> >Know More</a>
            </div>
          </div>  
        <%}%>
      </div>
      <div id="about" class="col-2">
        <div class="column">
        </div>
      </div>
    </div>
